-- Helpers

local function sendInvokeExact(name, args_tbl)
    if not INVOKE then return false, "invoke_remote_not_found" end
    local payload = { invoke_id = INVOKE_ID[name] or 1, name = name, args = args_tbl or {} }
    local ok, err = pcall(function() INVOKE:FireServer(payload) end)
    return ok, ok and "invoke" or tostring(err)
end

local function r(o, rad)
    local c = Instance.new("UICorner", o)
    c.CornerRadius = UDim.new(0, rad or 8)
    return c
end

local function addStroke(o, thickness, color, transparency)
    local s = Instance.new("UIStroke", o)
    s.Thickness = thickness or 1
    s.Transparency = transparency or 0.3
    s.Color = color or Color3.fromRGB(90, 100, 255)
    return s
end

local function parseNumber(txt)
    txt = tostring(txt or ""):gsub("[^%d%-%.]","")
    return tonumber(txt)
end

-- Style helpers (UI ONLY ‚Äì kh√¥ng ƒë·ªïi ch·ª©c nƒÉng)

local ACCENT      = Color3.fromRGB(116, 127, 255)
local ACCENT_SOFT = Color3.fromRGB(88, 101, 242)
local BG_DARK     = Color3.fromRGB(10, 12, 20)
local BG_MID      = Color3.fromRGB(16, 18, 28)
local BG_CARD     = Color3.fromRGB(22, 25, 40)
local BG_INPUT    = Color3.fromRGB(30, 32, 48)
local TXT_MAIN    = Color3.fromRGB(230, 235, 245)
local TXT_MUTED   = Color3.fromRGB(150, 160, 190)

local function styleTextBox(tb)
    tb.BackgroundColor3 = BG_INPUT
    tb.BorderSizePixel = 0
    tb.TextColor3 = TXT_MAIN
    tb.PlaceholderColor3 = TXT_MUTED
    tb.Font = Enum.Font.Gotham
    tb.TextSize = 11
    tb.ClearTextOnFocus = false
    tb.TextWrapped = false
    r(tb, 8)
    addStroke(tb, 1, Color3.fromRGB(60, 70, 120), 0.3)
end

local function stylePrimaryButton(btn)
    btn.BackgroundColor3 = ACCENT_SOFT
    btn.BorderSizePixel = 0
    btn.TextColor3 = Color3.new(1,1,1)
    btn.Font = Enum.Font.GothamSemibold
    btn.TextSize = 11
    btn.AutoButtonColor = false
    r(btn, 8)
    addStroke(btn, 1, Color3.fromRGB(140, 150, 255), 0.25)
end

local function styleSecondaryButton(btn)
    btn.BackgroundColor3 = BG_CARD
    btn.BorderSizePixel = 0
    btn.TextColor3 = TXT_MAIN
    btn.Font = Enum.Font.Gotham
    btn.TextSize = 11
    btn.AutoButtonColor = false
    r(btn, 8)
    addStroke(btn, 1, Color3.fromRGB(60, 70, 110), 0.4)
end

local function styleDangerButton(btn)
    btn.BackgroundColor3 = Color3.fromRGB(180, 70, 90)
    btn.BorderSizePixel = 0
    btn.TextColor3 = Color3.new(1,1,1)
    btn.Font = Enum.Font.GothamSemibold
    btn.TextSize = 11
    btn.AutoButtonColor = false
    r(btn, 8)
    addStroke(btn, 1, Color3.fromRGB(255, 120, 140), 0.25)
end

