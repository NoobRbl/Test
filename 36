    styleTextBox(moneyBox)

    local setBtn = Instance.new("TextButton", frame)
    setBtn.Size = UDim2.new(1,0,0,28)
    setBtn.Text = "SET MONEY"
    setBtn.BackgroundColor3 = Color3.fromRGB(70, 160, 100)
    setBtn.BorderSizePixel = 0
    setBtn.TextColor3 = Color3.new(1,1,1)
    setBtn.Font = Enum.Font.GothamSemibold
    setBtn.TextSize = 11
    r(setBtn,8)
    addStroke(setBtn,1,Color3.fromRGB(120,210,150),0.3)

    setBtn.MouseButton1Click:Connect(function()
        local val = parseNumber(moneyBox.Text)
        if not val then return er("‚ö†Ô∏è Invalid Amount") end

        local args = { [1] = val }
        local success, result = pcall(function()
            return RS.Packages.Net:FindFirstChild("RF/AdminService/GiveMoney"):InvokeServer(unpack(args))
        end)
        if success then
            ok("üí∞ GiveMoney Success ‚úÖ: "..val)
        else
            er("‚ùå GiveMoney Failed: "..tostring(result))
        end
    end)
end

