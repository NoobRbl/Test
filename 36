-- Tab system

local tabBtns = {}
local tabFrames = {}

local function createTab(name, icon, order)
    local btn = Instance.new("TextButton", sideTab)
    btn.Size = UDim2.new(1, -12, 0, 32)
    btn.BackgroundColor3 = BG_CARD
    btn.Text = icon .. "  " .. name
    btn.Font = Enum.Font.GothamSemibold
    btn.TextSize = 11
    btn.TextColor3 = TXT_MUTED
    btn.AutoButtonColor = false
    btn.BorderSizePixel = 0
    btn.LayoutOrder = order
    btn.TextXAlignment = Enum.TextXAlignment.Left
    r(btn, 10)
    addStroke(btn, 1, Color3.fromRGB(50, 60, 120), 0.5)

    local frame = Instance.new("ScrollingFrame", contentContainer)
    frame.Size = UDim2.new(1, -10, 1, -8)
    frame.Position = UDim2.fromOffset(5, 4)
    frame.BackgroundTransparency = 1
    frame.BorderSizePixel = 0
    frame.ScrollBarThickness = 4
    frame.ScrollBarImageColor3 = ACCENT
    frame.CanvasSize = UDim2.new(0, 0, 0, 0)
    frame.Visible = false
    frame.ClipsDescendants = true

    local pad = Instance.new("UIPadding", frame)
    pad.PaddingLeft   = UDim.new(0, 6)
    pad.PaddingRight  = UDim.new(0, 6)
    pad.PaddingTop    = UDim.new(0, 6)
    pad.PaddingBottom = UDim.new(0, 6)

    local layout = Instance.new("UIListLayout", frame)
    layout.Padding = UDim.new(0, 6)
    layout.SortOrder = Enum.SortOrder.LayoutOrder
    layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        frame.CanvasSize = UDim2.new(0, 0, 0, layout.AbsoluteContentSize.Y + 12)
    end)

    tabBtns[name] = btn
    tabFrames[name] = frame

    btn.MouseButton1Click:Connect(function()
        for n, b in pairs(tabBtns) do
            b.BackgroundColor3 = BG_CARD
            b.TextColor3 = TXT_MUTED
            if tabFrames[n] then
                tabFrames[n].Visible = false
            end
        end
        btn.BackgroundColor3 = ACCENT_SOFT
        btn.TextColor3 = Color3.new(1,1,1)
        frame.Visible = true
    end)

    return frame
end

-- Create tabs

local spawnerTab = createTab("Brainrot","üß†",1)
local eventTab   = createTab("Event","üéâ",2)
local moneyTab   = createTab("Money","üí∞",3)
local rebirthTab = createTab("Rebirth","üîÑ",4)
local luckyTab   = createTab("Lucky","üçÄ",5)
local otherTab   = createTab("Other‚öôÔ∏è","‚öôÔ∏è",6)

-- Auto select default tab (Brainrot)
do
    for n, b in pairs(tabBtns) do
        b.BackgroundColor3 = BG_CARD
        b.TextColor3 = TXT_MUTED
        if tabFrames[n] then
            tabFrames[n].Visible = false
        end
    end

    local defaultName = "Brainrot"
    if tabBtns[defaultName] and tabFrames[defaultName] then
        tabBtns[defaultName].BackgroundColor3 = ACCENT_SOFT
        tabBtns[defaultName].TextColor3 = Color3.new(1,1,1)
        tabFrames[defaultName].Visible = true
    end
end

------------------- PET SPAWNER (Brainrot) -------------------

do
    local frame = spawnerTab

    local nameBox = Instance.new("TextBox", frame)
    nameBox.Size = UDim2.new(1, 0, 0, 28)
    nameBox.PlaceholderText = "Pet Name"
    nameBox.Text = "Strawberry Elephant"
    styleTextBox(nameBox)

    local mutBox = Instance.new("TextBox", frame)
    mutBox.Size = UDim2.new(1, 0, 0, 28)
    mutBox.PlaceholderText = "Mutation (optional)"
    mutBox.Text = ""
    styleTextBox(mutBox)

    local function getTraitsString()
        return ""
    end

    local looping = false
    local loopThread

    local loopBtn = Instance.new("TextButton", frame)
    loopBtn.Size = UDim2.new(1, 0, 0, 26)
    loopBtn.Text = "Loop: OFF"
    styleSecondaryButton(loopBtn)

    local spawnBtn = Instance.new("TextButton", frame)
    spawnBtn.Size = UDim2.new(1, 0, 0, 28)
    spawnBtn.Text = "SPAWN BRAINROT üß†"
    stylePrimaryButton(spawnBtn)

    local function runSpecialStartEvent(brainrotName)
        local args
        if brainrotName == "Meowl" then
            args = {[1] = "Meowl"}
        elseif brainrotName == "Elephant" or brainrotName == "Strawberry Elephant" then
            args = {[1] = "Strawberry"}
        else
            return
        end
        pcall(function()
            RS.Packages.Net:FindFirstChild("RF/AdminService/StartEvent"):InvokeServer(unpack(args))
        end)
    end

    local function spawnPet()
        local name = tostring(nameBox.Text or "")
        if name == "" then return er("‚ö†Ô∏è Enter Pet Name") end

        local mutation = tostring(mutBox.Text or "")
        local mutArg = (mutation == "" or mutation == "Mutation (optional)") and "" or mutation
        local traitsArg = getTraitsString()

        local args = {
            [1] = name,
            [2] = mutArg ~= "" and mutArg or "",
            [3] = traitsArg ~= "" and traitsArg or ""
        }

        local ok_, res = pcall(function()
            RS.Packages.Net:FindFirstChild("RF/AdminService/SpawnBrainrot"):InvokeServer(unpack(args))
            runSpecialStartEvent(name)
        end)

        if ok_ then ok("‚úÖ Spawned: " .. name) else er("‚ùå " .. tostring(res)) end
    end

    spawnBtn.MouseButton1Click:Connect(spawnPet)

    loopBtn.MouseButton1Click:Connect(function()
        looping = not looping
        loopBtn.Text = looping and "Loop: ON" or "Loop: OFF"
        loopBtn.BackgroundColor3 = looping and Color3.fromRGB(80, 170, 110) or BG_CARD
        if looping then
            if loopThread and coroutine.status(loopThread) ~= "dead" then return end
            loopThread = coroutine.create(function()
                while looping do
                    spawnPet()
                    task.wait(0.1)
                end
            end)
            coroutine.resume(loopThread)
        end
    end)
end

------------------- LUCKY BLOCK -------------------

do
    local frame = luckyTab

    local nameBox = Instance.new("TextBox", frame)
    nameBox.Size = UDim2.new(1,0,0,28)
    nameBox.PlaceholderText = "üçÄ Lucky Block Spawner"
    nameBox.Text = "Secret Lucky Block"
    styleTextBox(nameBox)

    local mutBox = Instance.new("TextBox", frame)
    mutBox.Size = UDim2.new(1,0,0,28)
    mutBox.PlaceholderText = "Mutation (optional)"
    mutBox.Text = ""
    styleTextBox(mutBox)

    local looping = false
    local loopThread

    local loopBtn = Instance.new("TextButton", frame)
    loopBtn.Size = UDim2.new(1,0,0,26)
    loopBtn.Text = "Loop: OFF"
    styleSecondaryButton(loopBtn)

    local spawnBtn = Instance.new("TextButton", frame)
    spawnBtn.Size = UDim2.new(1,0,0,28)
    spawnBtn.Text = "SPAWN LUCKY BLOCK üçÄ"
    spawnBtn.BackgroundColor3 = Color3.fromRGB(70,180,130)
    spawnBtn.BorderSizePixel = 0
    spawnBtn.TextColor3 = Color3.new(1,1,1)
    spawnBtn.Font = Enum.Font.GothamSemibold
    spawnBtn.TextSize = 11
    r(spawnBtn,8)
    addStroke(spawnBtn,1,Color3.fromRGB(120,220,170),0.25)

    local function spawnBlock()
        local name = tostring(nameBox.Text or "")
        if name=="" then return er("‚ö†Ô∏è Enter Name") end

        local mutation = tostring(mutBox.Text or "")
        local mutArg = (mutation=="" or mutation=="Mutation (optional)") and "" or mutation

        local args = {
            [1]=name,
            [2]=mutArg~="" and mutArg or "",
            [3]=""
        }

        local ok_, res = pcall(function()
            RS.Packages.Net:FindFirstChild("RF/AdminService/SpawnBrainrot"):InvokeServer(unpack(args))
        end)

        if ok_ then ok("‚úÖ Spawned: "..name) else er("‚ùå "..tostring(res)) end
    end

    spawnBtn.MouseButton1Click:Connect(spawnBlock)

    loopBtn.MouseButton1Click:Connect(function()
        looping = not looping
        loopBtn.Text = looping and "Loop: ON" or "Loop: OFF"
        loopBtn.BackgroundColor3 = looping and Color3.fromRGB(80, 170, 110) or BG_CARD
        if looping then
            if loopThread and coroutine.status(loopThread)~="dead" then return end
            loopThread=coroutine.create(function()
                while looping do
                    spawnBlock()
                    task.wait(0.1)
                end
            end)
            coroutine.resume(loopThread)
        end
    end)
end

------------------- EVENT EXECUTOR -------------------

do
    local frame = eventTab
    local selectedEvent

    local dropBtn = Instance.new("TextButton", frame)
    dropBtn.Size = UDim2.new(1,0,0,28)
    dropBtn.Text = "Choose Event üëá"
    styleSecondaryButton(dropBtn)
    dropBtn.TextXAlignment = Enum.TextXAlignment.Left

    local dropFrame = Instance.new("Frame", gui)
    dropFrame.Size = UDim2.new(0,260,0,180)
    dropFrame.BackgroundColor3 = BG_MID
    dropFrame.BorderSizePixel = 0
    dropFrame.Visible = false
    dropFrame.ZIndex = 100
    r(dropFrame,10)
    addStroke(dropFrame,1,Color3.fromRGB(60,70,130),0.4)

    local dropScroll = Instance.new("ScrollingFrame", dropFrame)
    dropScroll.Size = UDim2.new(1,-6,1,-6)
    dropScroll.Position = UDim2.fromOffset(3,3)
    dropScroll.BackgroundTransparency = 1
    dropScroll.BorderSizePixel = 0
    dropScroll.ScrollBarThickness = 3
    dropScroll.ScrollBarImageColor3 = ACCENT
    dropScroll.ZIndex = 101

    local dropLayout = Instance.new("UIListLayout", dropScroll)
    dropLayout.Padding = UDim.new(0,2)

    for _, evtName in ipairs(EVENT_LIST) do
        local item = Instance.new("TextButton", dropScroll)
        item.Size = UDim2.new(1,-4,0,24)
        item.BackgroundColor3 = BG_CARD
        item.Text = evtName
        item.Font = Enum.Font.Gotham
        item.TextSize = 10
        item.TextColor3 = TXT_MAIN
        item.TextXAlignment = Enum.TextXAlignment.Left
        item.BorderSizePixel = 0
        item.ZIndex = 102
        r(item,6)
        addStroke(item,1,Color3.fromRGB(50,60,110),0.4)

        item.MouseButton1Click:Connect(function()
            selectedEvent = evtName
            dropBtn.Text = evtName.." ‚ñæ"
            dropFrame.Visible=false
        end)
    end

    dropLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        dropScroll.CanvasSize = UDim2.new(0,0,0,dropLayout.AbsoluteContentSize.Y+4)
    end)

    dropBtn.MouseButton1Click:Connect(function()
        dropFrame.Visible = not dropFrame.Visible
        if dropFrame.Visible then
            local pos = dropBtn.AbsolutePosition
            dropFrame.Position = UDim2.fromOffset(pos.X, pos.Y + dropBtn.AbsoluteSize.Y + 5)
        end
    end)

    local execAllBtn = Instance.new("TextButton", frame)
    execAllBtn.Size = UDim2.new(1,0,0,28)
    execAllBtn.Text = "‚ö° EXECUTE ALL EVENT"
    execAllBtn.BackgroundColor3 = Color3.fromRGB(140, 70, 190)
    execAllBtn.BorderSizePixel = 0
    execAllBtn.TextColor3 = Color3.new(1,1,1)
    execAllBtn.Font = Enum.Font.GothamSemibold
    execAllBtn.TextSize = 11
    r(execAllBtn,8)
    addStroke(execAllBtn,1,Color3.fromRGB(190,110,240),0.3)

    execAllBtn.MouseButton1Click:Connect(function()
        local phases = "Phase 5: Sammy Snap"
        local args = {[1]=phases}
        pcall(function()
            RS.Packages.Net:FindFirstChild("RF/AdminService/StartEvent"):InvokeServer(unpack(args))
        end)
        ok("‚úÖ Executed All Events")
    end)

    local execBtn = Instance.new("TextButton", frame)
    execBtn.Size = UDim2.new(1,0,0,28)
    execBtn.Text = "EXECUTE SELECTED"
    stylePrimaryButton(execBtn)

    execBtn.MouseButton1Click:Connect(function()
        if not selectedEvent then return er("‚ö†Ô∏è Choose Event First") end
        local args={[1]=selectedEvent}
        pcall(function()
            RS.Packages.Net:FindFirstChild("RF/AdminService/StartEvent"):InvokeServer(unpack(args))
        end)
        ok("‚úÖ Executed: "..selectedEvent)
    end)
end

