rebirthBox.TextSize = 14
Instance.new("UICorner", rebirthBox)

-- H√†m t·∫°o n√∫t
local function createButton(text, color, posX, posY)
	local btn = Instance.new("TextButton", main)
	btn.Size = UDim2.new(0.45, 0, 0, 30)
	btn.Position = UDim2.new(posX, 0, posY, 0)
	btn.BackgroundColor3 = color
	btn.Text = text
	btn.TextColor3 = Color3.new(1, 1, 1)
	btn.Font = Enum.Font.GothamBold
	btn.TextSize = 14
	btn.AutoButtonColor = false
	Instance.new("UICorner", btn)
	return btn
end

-- N√∫t ch·ª©c nƒÉng
local btnMoney = createButton("üí∞ Set Money", Color3.fromRGB(52, 199, 89), 0.05, 0.48)
local btnRebirth = createButton("üîÅ Set Rebirth", Color3.fromRGB(52, 152, 219), 0.5, 0.48)
local btnMorph = createButton("üß† Brainrot Morph", Color3.fromRGB(108, 92, 231), 0.05, 0.62)
local btnDup = createButton("üåÄ Duplicated", Color3.fromRGB(155, 89, 182), 0.5, 0.62)
local btnSpawner = createButton("üß¨ Brainrots Spawner", Color3.fromRGB(93, 173, 226), 0.05, 0.76)
local btnLucky = createButton("üé≤ Lucky Block Spawner", Color3.fromRGB(46, 204, 113), 0.5, 0.76)
local btnExec = createButton("‚ö° Execute All Event", Color3.fromRGB(241, 196, 15), 0.05, 0.9)

-- H√†m g·ª≠i ph√≠m
local function sendKey(key)
	VIM:SendKeyEvent(true, key, false, game)
	task.wait(0.1)
	VIM:SendKeyEvent(false, key, false, game)
end

-- H√†nh ƒë·ªông t·ª´ng n√∫t
btnSpawner.MouseButton1Click:Connect(function()
	sendKey(Enum.KeyCode.L)
end)

btnExec.MouseButton1Click:Connect(function()
	sendKey(Enum.KeyCode.R)
end)

btnMorph.MouseButton1Click:Connect(function()
	sendKey(Enum.KeyCode.Q)
end)

verified.MouseButton1Click:Connect(function()
	verified.Text = "‚úî Verified ‚úì"
	verified.BackgroundColor3 = Color3.fromRGB(56, 142, 60)
end)

toggle.MouseButton1Click:Connect(function()
	main.Visible = not main.Visible
end)
