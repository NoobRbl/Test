Instance.new("UICorner", verified)

-- √î nh·∫≠p Money
local moneyBox = Instance.new("TextBox", main)
moneyBox.Size = UDim2.new(0.45, 0, 0, 30)
moneyBox.Position = UDim2.new(0.05, 0, 0.35, 0)
moneyBox.PlaceholderText = "Enter money..."
