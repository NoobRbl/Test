--// SAB Admin Panel GUI (v4 by ChatGPT)

if game.CoreGui:FindFirstChild("SABPanel") then
	game.CoreGui.SABPanel:Destroy()
end

local VU = game:GetService("VirtualUser")
local VIM = game:GetService("VirtualInputManager")
local TweenService = game:GetService("TweenService")

local gui = Instance.new("ScreenGui", game.CoreGui)
gui.Name = "SABPanel"
gui.ResetOnSpawn = false

-- Khung ch√≠nh
local main = Instance.new("Frame", gui)
main.Size = UDim2.new(0, 380, 0, 340)
main.Position = UDim2.new(0.5, -190, 0.5, -170)
main.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
main.BorderSizePixel = 2
main.BorderColor3 = Color3.fromRGB(128, 0, 255)
main.Visible = false
main.Active = true
main.Draggable = true

local corner = Instance.new("UICorner", main)
corner.CornerRadius = UDim.new(0, 20)

-- Animation m·ªü
main.BackgroundTransparency = 1
main.Size = UDim2.new(0, 0, 0, 0)
main.Visible = true
TweenService:Create(main, TweenInfo.new(0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
	BackgroundTransparency = 0,
	Size = UDim2.new(0, 380, 0, 340)
}):Play()

-- Ti√™u ƒë·ªÅ
local title = Instance.new("TextLabel", main)
title.Size = UDim2.new(1, 0, 0, 30)
title.BackgroundTransparency = 1
title.Text = "Admin Command Script"
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.Font = Enum.Font.GothamBold
title.TextSize = 18

-- N√∫t ƒë√≥ng/m·ªü
local toggle = Instance.new("TextButton", main)
toggle.Size = UDim2.new(0, 25, 0, 25)
toggle.Position = UDim2.new(1, -30, 0, 5)
toggle.BackgroundColor3 = Color3.fromRGB(128, 0, 255)
toggle.Text = "X"
toggle.TextColor3 = Color3.new(1,1,1)
toggle.Font = Enum.Font.GothamBold
toggle.TextSize = 14
Instance.new("UICorner", toggle)

-- N√∫t Verified
local verified = Instance.new("TextButton", main)
verified.Size = UDim2.new(0.45, 0, 0, 30)
verified.Position = UDim2.new(0.05, 0, 0.15, 0)
verified.BackgroundColor3 = Color3.fromRGB(76, 175, 80)
verified.Text = "‚úî Verified"
verified.TextColor3 = Color3.new(1, 1, 1)
verified.Font = Enum.Font.GothamBold
