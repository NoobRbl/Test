--// SAB Admin Panel GUI (by ChatGPT)
-- Giao di·ªán gi·ªëng m·∫´u trong ·∫£nh b·∫°n g·ª≠i

-- X√≥a GUI c≈© n·∫øu c√≥
if game.CoreGui:FindFirstChild("SABPanel") then
	game.CoreGui.SABPanel:Destroy()
end

-- T·∫°o ScreenGui
local gui = Instance.new("ScreenGui", game.CoreGui)
gui.Name = "SABPanel"
gui.ResetOnSpawn = false

-- Khung ch√≠nh
local main = Instance.new("Frame", gui)
main.Size = UDim2.new(0, 380, 0, 340)
main.Position = UDim2.new(0.5, -190, 0.5, -170)
main.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
main.BorderSizePixel = 0
main.Active = true
main.Draggable = true
main.Name = "Main"

-- Ti√™u ƒë·ªÅ
local title = Instance.new("TextLabel", main)
title.Size = UDim2.new(1, 0, 0, 30)
title.BackgroundTransparency = 1
title.Text = "Admin Command Script"
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.Font = Enum.Font.GothamBold
title.TextSize = 18

-- N√∫t Verified
local verified = Instance.new("TextButton", main)
verified.Size = UDim2.new(0.45, 0, 0, 30)
verified.Position = UDim2.new(0.05, 0, 0.15, 0)
verified.BackgroundColor3 = Color3.fromRGB(76, 175, 80)
verified.Text = "‚úî Verified"
verified.TextColor3 = Color3.new(1, 1, 1)
verified.Font = Enum.Font.GothamBold
verified.TextSize = 16
verified.AutoButtonColor = false
verified.BackgroundTransparency = 0.1
verified.UICorner = Instance.new("UICorner", verified)

-- √î nh·∫≠p Money
local moneyBox = Instance.new("TextBox", main)
moneyBox.Size = UDim2.new(0.45, 0, 0, 30)
moneyBox.Position = UDim2.new(0.05, 0, 0.35, 0)
moneyBox.PlaceholderText = "Enter money..."
moneyBox.Text = ""
moneyBox.Font = Enum.Font.Gotham
moneyBox.TextSize = 14
moneyBox.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
moneyBox.TextColor3 = Color3.new(1, 1, 1)
Instance.new("UICorner", moneyBox)

-- √î nh·∫≠p Rebirth
local rebirthBox = Instance.new("TextBox", main)
rebirthBox.Size = UDim2.new(0.45, 0, 0, 30)
rebirthBox.Position = UDim2.new(0.5, 0, 0.35, 0)
rebirthBox.PlaceholderText = "Enter rebirth..."
rebirthBox.Text = ""
rebirthBox.Font = Enum.Font.Gotham
rebirthBox.TextSize = 14
rebirthBox.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
rebirthBox.TextColor3 = Color3.new(1, 1, 1)
Instance.new("UICorner", rebirthBox)

-- H√†m t·∫°o n√∫t nhanh
local function createButton(text, color, posX, posY)
	local btn = Instance.new("TextButton", main)
	btn.Size = UDim2.new(0.45, 0, 0, 30)
	btn.Position = UDim2.new(posX, 0, posY, 0)
	btn.BackgroundColor3 = color
	btn.Text = text
	btn.TextColor3 = Color3.new(1, 1, 1)
	btn.Font = Enum.Font.GothamBold
	btn.TextSize = 14
	btn.AutoButtonColor = false
	Instance.new("UICorner", btn)
	return btn
end

-- C√°c n√∫t ch·ª©c nƒÉng
local btnMoney = createButton("üí∞ Set Money", Color3.fromRGB(52, 199, 89), 0.05, 0.48)
local btnRebirth = createButton("üîÅ Set Rebirth", Color3.fromRGB(52, 152, 219), 0.5, 0.48)
local btnMorph = createButton("üß† Brainrot Morph", Color3.fromRGB(108, 92, 231), 0.05, 0.62)
local btnDup = createButton("üåÄ Duplicated", Color3.fromRGB(155, 89, 182), 0.5, 0.62)
local btnSpawner = createButton("üß¨ Brainrots Spawner", Color3.fromRGB(93, 173, 226), 0.05, 0.76)
local btnLucky = createButton("üé≤ Lucky Block Spawner", Color3.fromRGB(46, 204, 113), 0.5, 0.76)
local btnExec = createButton("‚ö° Execute All Event", Color3.fromRGB(241, 196, 15), 0.05, 0.9)

-- S·ª± ki·ªán click (b·∫°n th√™m script th·∫≠t v√†o ƒë√¢y)
btnMoney.MouseButton1Click:Connect(function()
	print("Set Money:", moneyBox.Text)
end)

btnRebirth.MouseButton1Click:Connect(function()
	print("Set Rebirth:", rebirthBox.Text)
end)

btnMorph.MouseButton1Click:Connect(function()
	print("Brainrot Morph pressed")
end)

btnDup.MouseButton1Click:Connect(function()
	print("Duplicated pressed")
end)

btnSpawner.MouseButton1Click:Connect(function()
	print("Brainrots Spawner pressed")
end)

btnExec.MouseButton1Click:Connect(function()
	print("Execute All Event pressed")
end)

btnLucky.MouseButton1Click:Connect(function()
	print("Lucky Block Spawner pressed")
end)

-- Verified n√∫t nh·∫•p hi·ªáu ·ª©ng
verified.MouseButton1Click:Connect(function()
	verified.Text = "‚úî Verified ‚úì"
	verified.BackgroundColor3 = Color3.fromRGB(56, 142, 60)
end)
